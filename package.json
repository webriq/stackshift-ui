{
  "name": "turborepo-template",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "pnpm next:pages",
    "doc": "node scripts/doc.js && typedoc",
    "format": "prettier --write \"**/*.{ts,tsx,md,css,scss}\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "preinstall": "node scripts/update-pm.js",
    "rebrand": "node scripts/rebrand.js",
    "next:pages": "pnpm --filter @stackshift-ui/next-pages dev",
    "dev:docs": "pnpm --filter @stackshift-ui/docs dev"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@stackshift-ui/typescript-config": "workspace:*",
    "@types/node": "^22.7.0",
    "enquirer": "^2.4.1",
    "plop": "^4.0.1",
    "prettier": "^3.3.3",
    "turbo": "^2.1.2",
    "typedoc": "^0.26.7",
    "typedoc-plugin-extras": "^3.1.0",
    "typedoc-plugin-inline-sources": "^1.1.0",
    "typedoc-plugin-mdn-links": "^3.3.1",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typedoc-plugin-rename-defaults": "^0.7.1",
    "typedoc-plugin-zod": "^1.2.1",
    "rimraf": "^3.0.2",
    "concurrently": "^7.6.0"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "eslint": "^8.0.0"
    }
  },
  "overrides": {
    "eslint": "^8.0.0"
  }
}